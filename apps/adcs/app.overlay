&i2c1 {
	status = "okay";

	bmi08x-gryo@68 {
		status = "okay";
		compatible = "bosch,bmi08x-gyro";
		reg = <0x68>;
		gyro-hz = "100_32";
		gyro-fs = <1000>;
	};

  bmi08x-accel@18 {
      status = "okay";
      compatible = "bosch,bmi08x-accel";
      reg = <0x18>;
      accel-hz = "100";
      accel-fs = <3>;
    };
};
